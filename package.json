{
  "name": "easydox",
  "version": "1.0.0",
  "description": "CLI Based tool to automate docs generation.",
  "main": "index.js",
  "scripts": {
    "dev-args": "npx ts-node src/bin/cli.ts -s example/ -f md,json,mdx -o example-docs",
    "dev-interactive": "npx ts-node src/bin/cli.ts",
    "build": "tsc",
    "start": "node dist/bin/cli.js ",
    "prestart": "npm run build",
    "format": "prettier --write ."
  },
  "bin": {
    "easydox": "./dist/bin/cli.js"
  },
  "keywords": [],
  "author": "Vikram Samak",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@babel/parser": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "doctrine": "^3.0.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "inquirer": "^12.4.2",
    "markdown-table": "^3.0.4",
    "openai": "^4.87.3",
    "prettier": "^3.5.3",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-lint": "^10.0.1",
    "remark-lint-heading-increment": "^4.0.1",
    "remark-normalize-headings": "^4.0.0",
    "remark-preset-lint-consistent": "^6.0.1",
    "remark-preset-lint-markdown-style-guide": "^6.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "remark-stringify": "^11.0.0"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.6",
    "@types/doctrine": "^0.0.9",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
